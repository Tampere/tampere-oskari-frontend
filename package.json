{
  "name": "oskari-frontend-tampere",
  "version": "1.1.0",
  "description": "kartat.tampere.fi/oskari bundles & app conf",
  "keywords": [
    "Oskari"
  ],
  "dependencies": {
    "oskari-frontend": "file:../oskari-frontend",
    "oskari-frontend-contrib": "file:../oskari-frontend-contrib"
  },
  "scripts": {
    "build": "node ./node_modules/oskari-frontend/node_modules/webpack/bin/webpack.js --config ./node_modules/oskari-frontend/webpack.config.js --mode production --progress --env.absolutePublicPath=true",
    "start": "node ./node_modules/oskari-frontend/node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./node_modules/oskari-frontend/webpack.config.js --hot --env.appdef=apps",
    "test": "eslint ."
  },
  "private": true,
  "devDependencies": {
    "eslint": "5.15.3",
    "eslint-config-standard": "12.0.0",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0"
  }
}
